{
  "manifest_version": 3,
  "name": "TestFlow",
  "version": "2.1.0",
  "description": "Web Testing Automation with Vision OCR and CDP Integration",
  "permissions": ["debugger","activeTab","tabs","storage","scripting"],
  "host_permissions": ["<all_urls>"],
  "web_accessible_resources": [{"resources": ["*.wasm", "*.traineddata"],"matches": ["<all_urls>"]}],
  "action": {"default_popup": "index.html","default_icon": {"16": "icons/icon16.png","48": "icons/icon48.png","128": "icons/icon128.png"}},
  "background": {"service_worker": "src/background/background.ts","type": "module"},
  "content_scripts": [{"matches": ["<all_urls>"],"js": ["src/content/content.ts"],"run_at": "document_idle"}],
  "icons": {"16": "icons/icon16.png","48": "icons/icon48.png","128": "icons/icon128.png"}
}
